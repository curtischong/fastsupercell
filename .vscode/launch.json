    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "python": "${workspaceFolder}/venv/bin/python" // Adjust the path if your venv is located elsewhere
        },
        {
            "name": "test",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/test.py",
            "console": "integratedTerminal",
            "python": "${workspaceFolder}/venv/bin/python", // Adjust the path if your venv is located elsewhere
            "justMyCode": false
        },
        {
            "name": "vis",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/vis_testing.py",
            "console": "integratedTerminal",
            "python": "${workspaceFolder}/venv/bin/python", // Adjust the path if your venv is located elsewhere
            "justMyCode": false
        }
    ]
}